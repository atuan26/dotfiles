  "black-formatter.args": ["--line-length=88"],
  "conventionalCommits.promptBody": false,
  "conventionalCommits.promptFooter": false,
  "conventionalCommits.scopes": ["build"],
  "conventionalCommits.showNewVersionNotes": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 8,
  "editor.fastScrollSensitivity": 5,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.indentation": true,
  "editor.hover.delay": 300,
  "editor.lineNumbers": "relative",
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.side": "right",
  "editor.occurrencesHighlight": "off",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "explorer.decorations.colors": false,
  "explorer.openEditors.visible": 1,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": true,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "glassit.alpha": 1,
  "go.toolsManagement.autoUpdate": true,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "liveServer.settings.donotShowInfoMsg": true,
  "powermode.combo.counterEnabled": "hide",
  "powermode.combo.location": "off",
  "powermode.combo.timerEnabled": "hide",
  "powermode.enabled": true,
  "powermode.shake.enabled": false,
  "python.defaultInterpreterPath": "\"${workspaceFolder}/.venv/bin/python\"",
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": true,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "tailwindCSS.classAttributes": ["class", "className"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Ubuntu (WSL)": {
      "args": ["-d", "Ubuntu"],
      "path": "C:\\Windows\\System32\\wsl.exe"
    }
  },
  "terminal.integrated.smoothScrolling": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  // "window.title": "",
  "update.mode": "start",
  "vim.easymotion": true,
  // For visual mode
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-n>": false,
    "<C-f>": false,
    "<C-j>": false,
    "<C-p>": false,
    "<C-v>": false,
    "<C-w>": false,
    "<C-x>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#a9dc7660",
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "k"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-l>"],
      "after": [
        "<c-g>",
        "u",
        "<Esc>",
        "[",
        "s",
        "1",
        "z",
        "=",
        "`",
        "]",
        "a",
        "<c-g>",
        "u"
      ]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindings": [
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<C-v>"]
    },
    {
      "before": ["<leader>", "m", "t"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "m", "l"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["workbench.action.quickTextSearch"]
    },
    {
      "before": ["<leader>", "f", "d"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "f", "i"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "<tab>"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "S"],
      "commands": ["git.stage"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "u"],
      "commands": ["git.unstage"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "g", "["],
      "commands": ["gitlens.diffWithPreviousInDiffRight"]
    },
    {
      "before": ["<leader>", "g", "]"],
      "commands": ["gitlens.diffWithNextInDiffRight"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"],
      "silent": true
    },
    {
      "before": ["[", "c"],
      "commands": [
        "workbench.action.editor.previousChange",
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": ["]", "c"],
      "commands": [
        "workbench.action.editor.nextChange",
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": ["[", "P"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["]", "P"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "p"],
      "commands": ["editor.action.marker.prev"]
    },
    //
    {
      "before": ["]", "p"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.saveWithoutFormatting"]
    },
    {
      "before": ["g", "l"],
      "commands": ["editor.action.openLink"]
    },
    {
      "before": ["g", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "q"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
    {
      // TODO: go to next, prev function
      "before": ["g", "f"],
      "after": [""]
    },
    {
      "before": ["g", "F"],
      "after": [""]
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<C-v>"]
    },
    {
      "before": ["<leader>", "f", "d"],
      "commands": ["editor.action.formatSelection"]
    }
  ],
