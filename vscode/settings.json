  "whichkey.delay": 200,
  "whichkey.bindings": [
    {
      "key": " ",
      "name": "Commands",
      "type": "command",
      "command": "workbench.action.showCommands"
    },
    {
      "key": "?",
      "name": "Search keybindings",
      "type": "command",
      "command": "whichkey.searchBindings"
    },
    {
      "key": "b",
      "name": "+Buffers/Editors",
      "type": "bindings",
      "bindings": [
        {
          "key": "c",
          "name": "Close active editor",
          "type": "command",
          "command": "workbench.action.closeActiveEditor"
        },
        {
          "key": "k",
          "name": "Close other editors",
          "type": "command",
          "command": "workbench.action.closeOtherEditors"
        }
      ]
    },
    {
      "key": "c",
      "name": "Close Active Editor",
      "type": "command",
      "command": "workbench.action.closeActiveEditor"
    },
    {
      "key": "k",
      "name": "Close Other Editor",
      "type": "command",
      "command": "workbench.action.closeOtherEditors"
    },
    {
      "key": "e",
      "name": "+Errors",
      "type": "bindings",
      "bindings": [
        {
          "key": ".",
          "name": "Error transient",
          "type": "command",
          "command": "whichkey.showTransient",
          "args": "whichkey.transient.error"
        },
        {
          "key": "l",
          "name": "List errors",
          "type": "command",
          "command": "workbench.actions.view.problems"
        },
        {
          "key": "n",
          "name": "Next error",
          "type": "command",
          "command": "editor.action.marker.next"
        },
        {
          "key": "p",
          "name": "Previous error",
          "type": "command",
          "command": "editor.action.marker.prev"
        }
      ]
    },
    {
      "key": "f",
      "name": "+File",
      "type": "bindings",
      "bindings": [
        {
          "key": "s",
          "name": "Search text in project",
          "type": "command",
          "command": "workbench.action.quickTextSearch"
        },
        {
          "key": "S",
          "name": "Save all files",
          "type": "command",
          "command": "workbench.action.files.saveAll"
        },
        {
          "key": "r",
          "name": "Rename file",
          "type": "commands",
          "commands": [
            "workbench.files.action.showActiveFileInExplorer",
            "renameFile"
          ]
        },
        {
          "key": "t",
          "name": "Show active file in tree/explorer view",
          "type": "command",
          "command": "workbench.files.action.showActiveFileInExplorer"
        },
        {
          "key": "y",
          "name": "Copy path of active file",
          "type": "command",
          "command": "workbench.action.files.copyPathOfActiveFile"
        },
        {
            "key": "i",
            "name": "Organize Imports",
            "type": "command",
            "command": "editor.action.organizeImports"
        },
        {
          "key": "f",
          "name": "Format file",
          "type": "command",
          "command": "editor.action.formatDocument"
        }
      ]
    },
    {
      "key": "g",
      "name": "+Git",
      "type": "bindings",
      "bindings": [
        {
          "key": "b",
          "name": "Checkout [b]ranch",
          "type": "command",
          "command": "git.checkout"
        },
        {
          "key": "c",
          "name": "Open Change",
          "type": "command",
          "command": "git.openChange"
        },
        {
          "key": "d",
          "name": "Delete Branch",
          "type": "command",
          "command": "git.deleteBranch"
        },
        {
          "key": "f",
          "name": "Fetch",
          "type": "command",
          "command": "git.fetch"
        },
        {
          "key": "i",
          "name": "Init",
          "type": "command",
          "command": "git.init"
        },
        {
          "key": "s",
          "name": "Stage selected",
          "type": "command",
          "command": "git.stageSelectedRanges"
        },
        {
            "key": "S",
            "name": "Stage file",
            "type": "command",
            "command": "git.stage"
        },
        {
            "key": "r",
            "name": "Revert selected",
            "type": "command",
            "command": "git.revertSelectedRanges"
        },        
        {
            "key": "u",
            "name": "Unstage file",
            "type": "command",
            "command": "git.unstage"
        },
        {
            "key": "[",
            "name": "Diff with Prev",
