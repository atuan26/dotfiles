{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "never"
    },
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "black-formatter.args": ["--line-length=88"],
  "conventionalCommits.promptBody": false,
  "conventionalCommits.promptFooter": false,
  "conventionalCommits.scopes": ["build"],
  "conventionalCommits.showNewVersionNotes": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 8,
  "editor.fastScrollSensitivity": 5,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.guides.highlightActiveBracketPair": true,
  "editor.guides.indentation": true,
  "editor.hover.delay": 300,
  "editor.lineNumbers": "relative",
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "mouseover",
  "editor.occurrencesHighlight": "off",
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "explorer.decorations.colors": false,
  "explorer.openEditors.visible": 1,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "extensions.ignoreRecommendations": true,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "glassit.alpha": 1,
  "go.toolsManagement.autoUpdate": true,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "liveServer.settings.donotShowInfoMsg": true,
  "powermode.combo.counterEnabled": "hide",
  "powermode.combo.location": "off",
  "powermode.combo.timerEnabled": "hide",
  "powermode.enabled": true,
  "powermode.shake.enabled": false,
  "python.defaultInterpreterPath": "\"${workspaceFolder}/.venv/bin/python\"",
  "search.showLineNumbers": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": true,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "tailwindCSS.classAttributes": ["class", "className"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "Ubuntu (WSL)",
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.profiles.windows": {
    "Command Prompt": {
      "args": [],
      "icon": "terminal-cmd",
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ]
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "PowerShell": {
      "icon": "terminal-powershell",
      "source": "PowerShell"
    },
    "Ubuntu (WSL)": {
      "args": ["-d", "Ubuntu"],
      "path": "C:\\Windows\\System32\\wsl.exe"
    }
  },
  "terminal.integrated.smoothScrolling": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "update.mode": "start",
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-b>": false,
    "<C-c>": false,
    "<C-n>": false,
    "<C-f>": false,
    "<C-j>": false,
    "<C-p>": false,
    "<C-v>": false,
    "<C-w>": false,
    "<C-x>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "#a9dc7660",
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [],
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindings": [
    {
      "after": ["g", "b"],
      "before": ["g", "l"]
    },
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    },
    {
      "after": ["H", "z", "z"],
      "before": ["H"]
    },
    {
      "after": ["L", "z", "z"],
      "before": ["L"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<C-v>"]
    },
    {
      "before": ["]", "b"],
      "commands": [":bn"]
    },
    {
      "before": ["[", "b"],
      "commands": [":bp"]
    },
    {
      "before": ["<leader>", "f", "w"],
      "commands": ["workbench.action.quickTextSearch"]
    },
    {
      "before": ["<leader>", "f", "d"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "f", "i"],
      "commands": ["editor.action.organizeImports"]
    },
    {
      "before": ["<leader>", "<tab>"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "S"],
      "commands": ["git.stage"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "u"],
      "commands": ["git.unstage"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "g", "["],
      "commands": ["gitlens.diffWithPreviousInDiffRight"]
    },
    {
      "before": ["<leader>", "g", "]"],
      "commands": ["gitlens.diffWithNextInDiffRight"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "R"],
      "commands": ["git.clean"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor"
        }
      ]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"],
      "silent": true
    },
    {
      "before": ["[", "c"],
      "commands": [
        "workbench.action.editor.previousChange",
        "workbench.action.compareEditor.previousChange"
      ]
    },
    {
      "before": ["]", "c"],
      "commands": [
        "workbench.action.editor.nextChange",
        "workbench.action.compareEditor.nextChange"
      ]
    },
    {
      "before": ["[", "P"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["]", "P"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "p"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "p"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.files.saveWithoutFormatting"]
    },
    {
      "before": ["g", "p", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "q"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
    {
      "before": ["g", "f"],
      "after": [""]
    },
    {
      "before": ["g", "F"],
      "after": [""]
    }
  ],
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindings": [
    {
      "after": ["g", "b"],
      "before": ["g", "l"]
    },
    {
      "after": ["\"", "+", "y"],
      "before": ["<C-c>"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["git.stageSelectedRanges"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "after": ["\"", "+", "p"],
      "before": ["<C-v>"]
    },
    {
      "before": ["<leader>", "f", "d"],
      "commands": ["editor.action.formatSelection"]
    }
  ],
  "workbench.colorCustomizations": {
    "statusBar.background": "#261d36",
    "statusBar.debuggingBackground": "#BF616A",
    "statusBar.debuggingForeground": "#434C5E",
    "statusBar.noFolderBackground": "#BF616A",
    "editorCursor.background": "#ff930f",
    "editorCursor.foreground": "#ff930f",
    "peekView.border": "#04fafa",
    "peekViewEditor.background": "#01041a",
    "peekViewResult.background": "#054c8a",
    "list.activeSelectionBackground": "#5b4582",
    "list.inactiveSelectionBackground": "#382a50",
    "list.focusBackground": "#261d36",
    "list.hoverBackground": "#2e1e4a",
    "terminalCursor.foreground": "#C45DFF",
    "sideBar.background": "#282a36",
    "tab.activeBorder": "#0000"
  },
  "workbench.colorTheme": "Dracula Theme",
  "workbench.editor.tabCloseButton": "off",
  "workbench.hover.delay": 1,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "zenMode.hideLineNumbers": false,
  "gitlens.changes.locations": ["gutter", "line", "overview"],
  "gitlens.hovers.changesDiff": "hunk",
  "workbench.editor.tabActionCloseVisibility": false,
  "git.showActionButton": {
    "commit": false,
    "publish": false,
    "sync": false
  },
  "scm.showActionButton": false,
  "scm.showInputActionButton": false,
  "git.showCommitInput": false,
  "git.defaultBranchName": "master",
  "scm.showOutgoingChanges": "never",
  "scm.showIncomingChanges": "never",
  "keyboard.dispatch": "keyCode",
  "vim.showMarksInGutter": true,
  "vim.foldfix": true,
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.sideBar.location": "right",
  "workbench.editor.showTabs": "none",
  "workbench.tips.enabled": false,
  "editor.showFoldingControls": "never",
  "editor.detectIndentation": false,
  "editor.fontSize": 14,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "git.decorations.enabled": false,
  "terminal.integrated.lineHeight": 1.5,
  "terminal.integrated.fontSize": 14,
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.editor.editorActionsLocation": "hidden",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "gitlens.views.searchAndCompare.avatars": false,
  "workbench.activityBar.location": "hidden",
  "gitlens.views.remotes.branches.layout": "list",
  "merge-conflict.diffViewPosition": "Beside",
  "vscode-color-picker.languages": [
    "json",
    "python",
    "javascript",
    "typescript"
  ],
  "git.openRepositoryInParentFolders": "never",
  "workbench.layoutControl.enabled": false,
  "workbench.layoutControl.type": "menu",
  "window.commandCenter": true,
  "editor.minimap.enabled": true,
  "workbench.view.showQuietly": {
    "workbench.panel.output": false
  },
  "explorer.compactFolders": false,
  "inlineChat.holdToSpeech": false,
  "workbench.navigationControl.enabled": false,
  "window.menuBarVisibility": "compact"
//   "roo-cline.allowedCommands": ["git log", "git diff", "git show"],
//   "roo-cline.deniedCommands": []
}
